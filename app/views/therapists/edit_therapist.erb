<h2><%=@therapist.first_name%><%=@therapist.last_name%><%=@therapist.license_type%></h2>

<form method="post" action="/therapists/<%= @therapist.id %>/edit">
<h2>Edit any of the following fields and press 'Update":</h2>
  <form action="/register" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <input type="text" name="username" placeholder="@therapist.username">
  <br></br>
  <input type="password" name="password" placeholder="Password">
  <br></br>
  <input type="text" name="first_name" placeholder="@therapist.first_name">
  <br></br>
  <input type="text" name="last_name" placeholder="@therapist.last_name">
  <br></br>
  <input type="text" name="license_type" placeholder="@therapist.license_type">
  <br></br>
  <input type="text" name="license_number" placeholder="@therapist.license_number">
  <br></br>
  <input type="submit" value="Update">
</form>


